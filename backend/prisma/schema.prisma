
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Empresa {
  id   String @id @default(auto())  @map("_id") @db.ObjectId
  nome String
}

model Oportunidade {
  id          String @id @default(auto())  @map("_id") @db.ObjectId
  titulo      String
  localizacao String
}





